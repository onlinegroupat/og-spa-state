{
  "name": "og-spa-state",
  "version": "1.0.0",
  "description": "Onlinegroup state management for single page applications",
  "main": "lib/main/index.js",
  "types": "lib/main/index.d.ts",
  "repository": "https://github.com/onlinegroupat/og-spa-state.git",
  "scripts": {
    "build": "tsc",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "mobx": "^3.1.9"
  },
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "jest": "^19.0.2",
    "ts-jest": "^19.0.10",
    "typescript": "^2.2.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/main/**/*.{ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      ".d.ts$"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "lcov"
    ],
    "testRegex": "src/test/.*.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
